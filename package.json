{
  "name": "grad-vue",
  "version": "0.1.0",
  "description": "Shared Vue.js components for Grad apps.",
  "type": "module",
  "main": "./dist/grad-vue.umd.js",
  "module": "./dist/grad-vue.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/grad-vue.es.js",
      "require": "./dist/grad-vue.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "keywords": [
    "vue",
    "vue3",
    "components",
    "graduate-college"
  ],
  "author": "Graduate College",
  "license": "MIT",
  "dependencies": {
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@types/node": "^25.0.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vue-tsc": "^3.1.8"
  }
}
